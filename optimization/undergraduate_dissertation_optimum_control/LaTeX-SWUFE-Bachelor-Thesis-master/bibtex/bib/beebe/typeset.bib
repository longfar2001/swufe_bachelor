@Article{Silver:1993:PRW,
  author =       "N. Clayton Silver and Curt C. Braun",
  title =        "Perceived readability of warning labels with varied
                 font sizes and styles",
  journal =      "Safety Science",
  volume =       "16",
  number =       "5-6",
  pages =        "615--625",
  month =        aug,
