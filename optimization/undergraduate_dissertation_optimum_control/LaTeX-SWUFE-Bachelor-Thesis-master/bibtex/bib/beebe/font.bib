@Article{Grebert:1992:CGP,
  author =       "Igor Grebert and David G. Stork and Ron Keesing and
                 Steve Mims",
  title =        "Connectionist generalization for production: an
                 example from gridfont",
  journal =      j-NEURAL-NETWORKS,
  volume =       "5",
  number =       "4",
  pages =        "699--710",
  month =        jul # "--" # aug,
  year =         "1992",
  CODEN =        "NNETEB",
