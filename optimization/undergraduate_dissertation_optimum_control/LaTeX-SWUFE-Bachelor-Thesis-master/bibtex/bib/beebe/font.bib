  title =        "Word spotting in scanned images using hidden {Markov}
                 models",
  journal =      "Proceedings --- ICASSP, IEEE International Conference
                 on Acoustics, Speech and Signal Processing",
  volume =       "5",
  pages =        "V-1--V-4",
  year =         "1993",
  CODEN =        "IPRODJ",
  ISBN =         "0-7803-0946-4",
