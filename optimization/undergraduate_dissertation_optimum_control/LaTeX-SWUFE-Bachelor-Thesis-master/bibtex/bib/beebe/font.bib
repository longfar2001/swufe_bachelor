@Article{Karow:1991:DPC,
  author =       "Peter Karow",
  title =        "Digital punch cutting",
  journal =      j-EPODD,
  volume =       "4",
  number =       "3",
  pages =        "151--170",
  month =        sep,
  year =         "1991",
  CODEN =        "EPODEU",
