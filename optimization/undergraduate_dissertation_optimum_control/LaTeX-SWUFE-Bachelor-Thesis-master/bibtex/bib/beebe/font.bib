@Article{Shamir:2003:CBA,
  author =       "Ariel Shamir",
  title =        "Constraint-based approach for automatic hinting of
                 digital typefaces",
  journal =      j-TOG,
  volume =       "22",
  number =       "2",
  pages =        "131--151",
  month =        apr,
