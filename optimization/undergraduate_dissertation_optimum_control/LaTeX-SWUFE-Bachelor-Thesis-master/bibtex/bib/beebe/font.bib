@Article{Khan:1987:RPC,
  author =       "Simon Khan and Theo Pavlidis and Henry S. Baird",
  title =        "On the Recognition of Printed Characters of any Font
                 and Size",
  journal =      "IEEE Transactions on Pattern Analysis and Machine
